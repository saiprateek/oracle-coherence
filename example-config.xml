<?xml version=”1.0″ encoding=”UTF-8″?>
<cache-config>
	<caching-scheme-mapping>
		<cache-mapping>
			<cache-name>prateek-cache</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
</caching-scheme-mapping>

<caching-schemes>
	<distributed-scheme>
		<scheme-name>distributed</scheme-name>
		<service-name>DistributedCache</service-name>
		<backing-map-scheme>
			<local-scheme/>
		</backing-map-scheme>
		<autostart>true</autostart>
	</distributed-scheme>
</caching-schemes>

</cache-config>